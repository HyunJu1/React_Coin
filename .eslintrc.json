{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["eslint:recommended","plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"off",
        "linebreak-style": 0,
        "react/prop-types":0,
        "indent": [
            "error",
            2
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}